        "foregroundImage": "./assets/BluetoothIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.dichterdev.esp32bluetoothremote"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "02b0eb41-a6d9-4879-989a-5a157ff1c9c5"
      }
    },
    "owner": "dichterdev"
  }
